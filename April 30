1)

#include <stdio.h>
#include <stdlib.h>
int sum(int a, int b)
{
    return a + b;
}
int main()
{
    int add = sum(10,30);
    printf("Sum is: %d", add);
    return 0;
}

2)

#include <math.h>
#include <stdio.h>
int main()
{
  double Number;
  Number = 49;
  double squareRoot = sqrt(Number);
  printf("The Square root of %.2lf = %.2lf", 
          Number, squareRoot);
  return 0;
}

3)

#include <stdio.h>
int sum(int a, int b) 
{ 
  return a + b; 
}
int main()
{
  int a = 30, b = 40;
  int res = sum(a, b);
  printf("Sum is: %d", res);
  return 0;
}

