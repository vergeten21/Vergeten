#include <stdio.h>
#include <stdlib.h>
main()
{
    int i;
    for(i=0;i<10;i++)
    {
        if(i==5)
            break;
        else
            printf("%d",i);    
    }
}


3)

#include <stdio.h>
#include <stdlib.h>
main()
{
    int i=0;
    while(1)
    {
        if(i==5)
            break;
        else
            printf("%d",i);
        i++;        
    }
}

4)

#include <stdio.h>
#include <stdlib.h>
main()
{
    int i;
    i=0;
ABC:
    if(i==10)
        goto XYZ;
    printf("%d",i);
    i++;
    goto ABC;
XYZ:
    return;          
}
